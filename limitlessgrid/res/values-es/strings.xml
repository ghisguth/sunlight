    <string name="settings_use_one_framebuffer_title">Usar un solo framebuffer</string>
    <string name="settings_use_one_framebuffer_summary_on">Usa una textura de renderizado y un framebuffer. Consume menos recursos, pero puede no funcionar en algunos dispositivos.</string>
    <string name="settings_use_one_framebuffer_summary_off">Usa dos texturas de renderizado y framebuffers. Consume más recursos, pero funcionará en todos los dispositivos.</string>
    <string name="settings_use_non_square_textures_title">Usar texturas no cuadradas</string>
    <string name="settings_use_non_square_textures_summary_on">Utilizará texturas rectangulares. Mejor rendimiento, pero puede no funcionar en algunos dispositivos antiguos.</string>
    <string name="settings_use_non_square_textures_summary_off">Usar solo texturas cuadradas. Consume más recursos, pero funcionará en todos los dispositivos.</string>
    <string name="settings_use_non_power_of_two_textures_title">Usar texturas que no sean potencia de dos</string>
    <string name="settings_use_non_power_of_two_textures_summary_on">Usa texturas que no sean potencias de dos. Mejor rendimiento/visual, pero puede no funcionar en algunos dispositivos.</string>
    <string name="settings_use_non_power_of_two_textures_summary_off">Usar solo texturas que sean potencias de dos. Consume más recursos, pero funcionará en todos los dispositivos.</string>
    <string name="settings_use_smaller_textures_title">Reducir el tamaño del framebuffer interno</string>
    <string name="settings_use_smaller_textures_summary_on">Utilizará texturas de renderizado más pequeñas. Mejor rendimiento, pero puede no verse tan bien como en resolución completa.</string>
    <string name="settings_use_smaller_textures_summary_off">Usar resolución de textura completa.</string>
